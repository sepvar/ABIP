\newcommand{\twoup}[5]{% 1=sizeof-vector, 2=label, 3=sizeof-vector, 4=label, 5=color-adjust
        \addtocounter{centy}{2}
        \addtocounter{centx}{-#5}
        \put(\thecentx,\thecenty){\circle*{1}}
%
        \addtocounter{centx}{-5}
        \setcounter{lblx}{\thecentx}\addtocounter{lblx}{-10}
        \setcounter{lbly}{\thecenty}\addtocounter{lbly}{#1}\addtocounter{lbly}{-5}
        \put(\thecentx,\thecenty){\vector(0,1){#1}}
        \put(\thelblx,\thelbly){\tiny #2}
        \addtocounter{centx}{5}
%
        \addtocounter{centx}{5}
        \setcounter{lblx}{\thecentx}\addtocounter{lblx}{3}
        \setcounter{lbly}{\thecenty}\addtocounter{lbly}{#3}\addtocounter{lbly}{-5}
        \put(\thecentx,\thecenty){\vector(0,1){#3}}
        \put(\thelblx,\thelbly){\tiny #4}
        \addtocounter{centx}{-5}
%
        \addtocounter{centx}{#5}
        \addtocounter{centy}{-2}}
\newcommand{\twodo}[5]{% 1=sizeof-vector, 2=label, 3=sizeof-vector, 4=label, 5=color-adjust
